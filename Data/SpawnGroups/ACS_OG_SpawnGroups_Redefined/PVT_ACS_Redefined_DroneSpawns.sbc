			[NpcIonThrustPowerMultiply:0.2]
			[ConfigureSpecialNpcThrusters:true]
			[RestrictNpcIonThrust:false]
			
			[UseRivalAi:true]
			[RivalAiReplaceRemoteControl:true]

			</Description>
			<Icon>Textures\GUI\Icons\Fake.dds</Icon>  <!--Do Not Edit-->
			<IsPirate>true</IsPirate>
			<Frequency>5.0</Frequency>
			<Prefabs>
				<Prefab SubtypeId="(NPC-ACS) Firefly Drone">
					<Position>
						<X>0.0</X>
						<Y>0.0</Y>
						<Z>0.0</Z>
					</Position>
					<Speed>25</Speed>
					<Behaviour>PVTACS-RD-DroneSmall-Behavior-100K</Behaviour>
				</Prefab>
			</Prefabs>
		</SpawnGroup>
<!-- 100K Bounties End -->	

	
