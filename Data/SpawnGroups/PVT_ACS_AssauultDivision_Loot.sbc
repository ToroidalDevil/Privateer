	<!-- Re-define existing ACS lootGroups to include location information -->
		<EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition"> <!-- ACS-LootGroupSpace -->
			<Id>
				<TypeId>Inventory</TypeId>
				<SubtypeId>ACS-LootGroupSpace</SubtypeId> 
			</Id>
			<Description>
				[MES Loot Group]
				
				[LootProfiles:(NPC-ACS)-ArmoryLootInv]
				[LootProfiles:(NPC-ACS)-CockpitLootInv]
				[LootProfiles:(NPC-ACS)-GenericLootInv]
				[LootProfiles:(NPC-ACS)-LockerLootInv]
				[LootProfiles:(NPC-ACS)-ThrusterLootSpaceInv]
				[LootProfiles:PVT-ACS-Loot-DataPads]
			</Description>
		</EntityComponent>
