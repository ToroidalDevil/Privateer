        [MatchSenderReceiverOwners:true]
		[CommandDelayTicks:1800]
      </Description>
    </EntityComponent>
	<!-- 
	This command is supposed to fire 30 seconds after the first escort request is processed. 
	This will turn off all Escort requests from all potential escorts. This turns off retreat triggers 
	that we do not want the escorts to have if they have been assigned. This results in any batch of newly 
	spawned escorts having 30 seconds to sign up, before they retreat.
	-->
<!-- COMMANDS End -->


